{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console) - net5.0",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Web App: Dotnet Build Debug - net5.0",
      "program": "${workspaceRoot}/src/OrchardCore.Cms.Web/bin/Debug/net5.0/OrchardCore.Cms.Web.dll",
      "args": [],
      "cwd": "${workspaceRoot}/src/OrchardCore.Cms.Web",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Launch (web debug) - net5.0",
      "type": "coreclr",
      "request": "launch",
      "internalConsoleOptions": "openOnSessionStart",
      "preLaunchTask": "Web App: Dotnet Build Debug - net5.0",
      "program": "${workspaceRoot}/src/OrchardCore.Cms.Web/bin/Debug/net5.0/OrchardCore.Cms.Web.dll",
      "args": [],
      "cwd": "${workspaceRoot}/src/OrchardCore.Cms.Web",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:5001;http://localhost:5000"
      },
      "stopAtEntry": false,
      "justMyCode": false,
      "launchBrowser": {
        "enabled": true,
        "args": "${auto-detect-url}",
        "windows": {
            "command": "cmd.exe",
            "args": "/C start ${auto-detect-url}"
        },
        "osx": {
            "command": "open"
        },
        "linux": {
            "command": "xdg-open"
        }
      }
    },
    {
      "name": ".NET Core Launch (web release) - net5.0",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Web App: Dotnet Build Release - net5.0",
      "program": "${workspaceRoot}/src/OrchardCore.Cms.Web/bin/Release/net5.0/OrchardCore.Cms.Web.dll",
      "args": [],
      "cwd": "${workspaceRoot}/src/OrchardCore.Cms.Web",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": true,
        "args": "${auto-detect-url}",
        "windows": {
            "command": "cmd.exe",
            "args": "/C start ${auto-detect-url}"
        },
        "osx": {
            "command": "open"
        },
        "linux": {
            "command": "xdg-open"
        }
      },
    },
    {
        "name": ".NET Core Attach",
        "type": "coreclr",
        "request": "attach",
        "processId": "${command:pickProcess}"
    },
    {
      "name": ".NET Core Launch (publish framework dependent) - net5.0",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Web App: Dotnet Publish (framework dependent) - net5.0",
      "program": "${workspaceRoot}/.build/release/OrchardCore.Cms.Web.dll",
      "args": [],
      "cwd": "${workspaceRoot}/.build/release",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": true,
        "args": "${auto-detect-url}",
        "windows": {
            "command": "cmd.exe",
            "args": "/C start ${auto-detect-url}"
        },
        "osx": {
            "command": "open"
        },
        "linux": {
            "command": "xdg-open"
        }
      },
    },
    {
      "name": ".NET Core Launch (publish self-contained) - net5.0",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Web App: Dotnet Publish (self-contained) - net5.0",
      "program": "${workspaceRoot}/.build/release/OrchardCore.Cms.Web.dll",
      "args": [],
      "cwd": "${workspaceRoot}/.build/release",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": true,
        "args": "${auto-detect-url}",
        "windows": {
            "command": "cmd.exe",
            "args": "/C start ${auto-detect-url}"
        },
        "osx": {
            "command": "open"
        },
        "linux": {
            "command": "xdg-open"
        }
      },
    },
    {
      "name": "Gulp Debug",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": [
        "--nolazy"
      ],
      "console": "internalConsole"
    }
  ]
}
